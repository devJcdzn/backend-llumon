{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "20.1.0",
  "compilerOptions": {
    "module": "node16",
    "moduleResolution": "node16",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "allowJs": true,
    "resolveJsonModule": true,
    "paths": {
      "@types": [
        "./src/types"
      ]
    }
  },
  "rootDir": ".",
}